<apex:page >
<apex:form >
<h1> Simple Form with Apex:Output Tags </h1> <br/><br/>
<h2> Please Fill the below details </h2> <br/><br/>

<apex:outputLabel value="Enter name : " for="name"></apex:outputLabel>
<apex:inputText id="name"/> <br/><br/>

<apex:outputLabel value="Enter Username : " for="username"/>
<apex:inputtext id="username"/> <br/><br/>

<apex:outputLabel value="Enter Password : " for="password"></apex:outputLabel>
<apex:inputSecret id="password"/> <br/><br/>

<apex:outputLabel value="Currently Employe : " for="emp"> </apex:outputLabel>
<apex:inputCheckbox id="emp"/> <br/><br/>

<apex:outputLink value="https://www.salesforce.com/in/"> Salesforce</apex:outputLink> <br/><br/>

<apex:commandButton value="Save"/>

</apex:form>
</apex:page>