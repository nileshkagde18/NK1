<!-- Start of Apex Page -->
<apex:page sidebar="true" showHeader="true">
<!-- Button should be in form tag so add form tag-->
<apex:form >
  <!-- Header from H1 to H6 (Display on top of page) --> 
  <h1> Welcome in Setup Build</h1>   <br></br>
  
  <!-- Page Block 1 -->
  <apex:pageblock title="first block">
  <!-- Button tag at top -->
  <apex:commandButton value="Save"/>
  <!-- 1st section of Block 1 -->
  <apex:pageBlockSection title="1st section"> 
  <!-- Item of 1st section (default colom size is 2) -->
  <apex:pageblockSectionItem > 1.1 no item </apex:pageblockSectionItem>
  <apex:pageblockSectionItem > 1.2 no item </apex:pageblockSectionItem>
  <apex:pageblockSectionItem > 1.3 no item </apex:pageblockSectionItem>
  <apex:pageblockSectionItem > 1.4 no item </apex:pageblockSectionItem>
  <apex:pageblockSectionItem > 1.5 no item </apex:pageblockSectionItem>
  <!-- End of 1st section -->
  </apex:pageBlockSection>   
  
  <!-- 2nd section of Block 1 -->
  <apex:pageblockSection title="2nd section">
  <!-- Item of 2nd section (default colom size is 2) -->
  <apex:pageblockSectionItem > 1 no item </apex:pageblockSectionItem>
  <apex:pageblockSectionItem > 2 no item </apex:pageblockSectionItem>
  <apex:pageblockSectionItem > 3 no item </apex:pageblockSectionItem>
  <!-- End of 2nd section -->
  </apex:pageblockSection>
   <!-- Button tag at Bottom -->
  <apex:commandButton value="Cancel"/>
  
  <!-- To craete Button at top and Bottom by using single Command tag -->
  <!-- write command tag inside pageblockbuttons tag -->
  <apex:pageBlockButtons >
  <apex:commandButton value="Add"/>
  <apex:commandButton value="Sub"/>
  </apex:pageBlockButtons>
  <!-- End of Page Block 1 -->
  </apex:pageblock>
  
   <!-- Page Block 2 -->
  <apex:pageblock title="second block">
  <!-- 1st section of Block 2 -->
  <apex:pageBlockSection title="2nd's 1st section"> 
  <!-- Item of 1st section (default colom size is 2) -->
  <apex:pageblockSectionItem > 2.1.1 no item </apex:pageblockSectionItem>
   <!-- End of 1st section of Block 2 -->
   </apex:pageblocksection>
   <!-- End of Page Block 2 -->
   </apex:pageblock>
 
  <!-- End Default Content REMOVE THIS -->  
 This is your new Page in Setup Build  

<!-- End of form tag -->
</apex:form>
<!-- End of Apex Page --> 
</apex:page>